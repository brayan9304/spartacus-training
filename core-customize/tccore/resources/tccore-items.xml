<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="items.xsd">

    <collectiontypes>
    </collectiontypes>

    <enumtypes>
    </enumtypes>

    <relations>
        <relation code="Customer2ReferredCustomerRel" localized="false">
            <sourceElement type="Customer" qualifier="customer" cardinality="one"/>
            <!--suppress TSVBased -->
            <targetElement type="ReferredCustomer" qualifier="referredCustomers" cardinality="many"
                           collectiontype="list"/>
        </relation>

        <relation code="Product2CustomProductList" localized="false">
            <deployment table="Product2CustomList" typecode="12003"/>
            <sourceElement type="Product" qualifier="product" cardinality="many"/>
            <targetElement type="CustomProductList" qualifier="customProductList" cardinality="many"/>
        </relation>

        <relation code="Customer2CustomProductList" localized="false">
            <sourceElement type="Customer" qualifier="customer" cardinality="one"/>
            <targetElement type="CustomProductList" qualifier="customProductList" cardinality="many"/>
        </relation>

        <relation code="FooterSocialLinkContainer2FooterSocialLink" generate="true" localized="false" autocreate="true">
            <sourceElement type="FooterSocialLinkContainer" qualifier="linkContainer" cardinality="one"/>
            <targetElement type="FooterSocialLink" qualifier="links" cardinality="many" ordered="true"/>
        </relation>

        <relation code="CardContainer2ItemComponent" generate="true" localized="false" autocreate="true">
            <sourceElement type="CardContainer" qualifier="card" cardinality="one"/>
            <targetElement type="ItemComponent" qualifier="items" cardinality="many" ordered="true"/>
        </relation>

        <relation code="ContainerPage2CustomCardContainer" generate="true" localized="false" autocreate="true">
            <sourceElement type="ContainerPage" qualifier="containerPage" cardinality="one"/>
            <targetElement type="CustomCardContainer" qualifier="cards" cardinality="many" ordered="true"/>
        </relation>

        <relation code="CustomCardContainer2ItemComponent" generate="true" localized="false" autocreate="true">
            <sourceElement type="CustomCardContainer" qualifier="customCard" cardinality="one"/>
            <targetElement type="ItemComponent" qualifier="page2Items" cardinality="many" ordered="true"/>
        </relation>

        <relation code="ContainerPage2SidebarContainer" generate="true" localized="false" autocreate="true">
            <sourceElement type="ContainerPage" qualifier="containerPage" cardinality="one"/>
            <targetElement type="SidebarContainer" qualifier="sidebars" cardinality="many" ordered="true"/>
        </relation>

        <relation code="SidebarContainer2SidebarLink" generate="true" localized="false" autocreate="true">
            <sourceElement type="SidebarContainer" qualifier="sidebarContainer" cardinality="one"/>
            <targetElement type="SidebarLink" qualifier="links" cardinality="many" ordered="true"/>
        </relation>


    </relations>

    <itemtypes>
        <typegroup name="CMS">
            <itemtype code="TcSplitViewBannerComponent" extends="AbstractResponsiveBannerComponent"
                      jaloclass="com.taloscommerce.core.jalo.TcSplitViewBannerComponent">
                <attributes>
                    <attribute qualifier="title" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="content" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>

        <typegroup name="Training Core Items">
            <itemtype code="DocumentType">
                <deployment table="DocumentTypes" typecode="12000"/>
                <attributes>
                    <attribute qualifier="code" type="java.lang.String">
                        <modifiers unique="true" initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="name" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
                <indexes>
                    <index name="codeIdx" unique="true">
                        <key attribute="code"/>
                    </index>
                </indexes>
            </itemtype>

            <itemtype code="ReferredCustomer">
                <deployment table="ReferredCustomers" typecode="12001"/>
                <attributes>
                    <attribute qualifier="email" type="java.lang.String">
                        <modifiers unique="true" initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="documentType" type="DocumentType">
                        <modifiers initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="documentNumber" type="java.lang.String">
                        <modifiers initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="firstName" type="java.lang.String">
                        <modifiers initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="lastName" type="java.lang.String">
                        <modifiers initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
                <indexes>
                    <index name="emailIdx" unique="true">
                        <key attribute="email"/>
                    </index>
                </indexes>
            </itemtype>
        </typegroup>

        <typegroup name="Electronics">
            <itemtype code="ElectronicsColorVariantProduct" extends="VariantProduct"
                      jaloclass="com.taloscommerce.core.jalo.ElectronicsColorVariantProduct">
                <attributes>
                    <attribute qualifier="color" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color of the product.</description>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>
        <typegroup name="CustomList">
            <itemtype code="CustomProductList" extends="GenericItem">
                <deployment table="CustomProductList" typecode="12002"/>
                <attributes>
                    <attribute qualifier="id" type="java.lang.String">
                        <description>Id of the list</description>
                        <modifiers optional="false" initial="true" unique="true"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="name" type="java.lang.String">
                        <description>Name of the list</description>
                        <modifiers initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="description" type="java.lang.String">
                        <description>Description of the list</description>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>


        <typegroup name="CustomFooter">
            <itemtype code="CustomFooterNavNode" extends="CMSNavigationNode">
                <attributes>
                    <attribute qualifier="media" type="localized:Media">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="CustomFooterBottom" extends="FooterNavigationComponent">
            </itemtype>
        </typegroup>
        <typegroup name="footerSocialNetwork">
            <itemtype code="FooterSocialLink" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.FooterSocialLink"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="url" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="text" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="media" type="Media">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

           <itemtype code="FooterSocialLinkContainer" extends="SimpleCMSComponent"
                     jaloclass="com.taloscommerce.core.jalo.FooterSocialLinkContainer"
                      autocreate="true" generate="true">
            </itemtype>
        </typegroup>

        <typegroup name="extraPages">
            <itemtype code="ItemComponent" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.ItemComponent"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="title" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="media" type="Media">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="cardType" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="CardContainer" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.CardContainer"
                      autocreate="true" generate="true">
            </itemtype>

            <!-- New card container -->
            <itemtype code="CustomCardContainer" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.CustomCardContainer"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="title" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="id" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
            <!-- End New Card Container -->

            <!-- New container -->
            <itemtype code="ContainerPage" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.ContainerPage"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="description" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
            <!-- End New Container -->

            <!-- Sidebar container -->
            <itemtype code="SidebarContainer" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.SidebarContainer"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="title" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
            <!-- End Sidebar container -->

            <!-- Sidebar link -->
            <itemtype code="SidebarLink" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.SidebarLink"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="title" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="level" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="link" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
            <!-- End Sidebar link -->



            <itemtype code="CustomItemComponent" extends="ItemComponent"
                      jaloclass="com.taloscommerce.core.jalo.CustomItemComponent"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="description" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="position" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>
    </itemtypes>
</items>
