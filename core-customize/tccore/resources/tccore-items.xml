<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="items.xsd">

    <collectiontypes>
    </collectiontypes>

    <enumtypes>
    </enumtypes>

    <relations>
        <relation code="Customer2ReferredCustomerRel" localized="false">
            <sourceElement type="Customer" qualifier="customer" cardinality="one"/>
            <!--suppress TSVBased -->
            <targetElement type="ReferredCustomer" qualifier="referredCustomers" cardinality="many"
                           collectiontype="list"/>
        </relation>

        <relation code="Product2CustomProductList" localized="false">
            <deployment table="Product2CustomList" typecode="12003"/>
            <sourceElement type="Product" qualifier="product" cardinality="many"/>
            <targetElement type="CustomProductList" qualifier="customProductList" cardinality="many"/>
        </relation>

        <relation code="Customer2CustomProductList" localized="false">
            <sourceElement type="Customer" qualifier="customer" cardinality="one"/>
            <targetElement type="CustomProductList" qualifier="customProductList" cardinality="many"/>
        </relation>

        <relation code="FooterSocialLinkContainer2FooterSocialLink" generate="true" localized="false" autocreate="true">
            <sourceElement type="FooterSocialLinkContainer" qualifier="linkContainer" cardinality="one"/>
            <targetElement type="FooterSocialLink" qualifier="links" cardinality="many" ordered="true"/>
        </relation>

        <relation code="SimpleItemContainer2SimpleItem" generate="true" localized="false" autocreate="true">
            <sourceElement type="SimpleItemContainer" qualifier="itemContainer" cardinality="one"/>
            <targetElement type="SimpleItem" qualifier="items" cardinality="many" ordered="true"/>
        </relation>

        <relation code="CardComponent2SimpleItemComponent" generate="true" localized="false" autocreate="true">
            <sourceElement type="CardComponent" qualifier="card" cardinality="one"/>
            <targetElement type="SimpleItemComponent" qualifier="items" cardinality="many" ordered="true"/>
        </relation>

        <relation code="CustomBannerComponent2CustomSimpleItemComponent" generate="true" localized="false" autocreate="true">
            <sourceElement type="CustomBannerComponent" qualifier="bannerContainer" cardinality="one"/>
            <targetElement type="CustomSimpleItemComponent" qualifier="containerItems" cardinality="many" ordered="true"/>
        </relation>

        <relation code="CustomIIBannerComponent2CustomSimpleItemComponent" generate="true" localized="false" autocreate="true">
            <sourceElement type="CustomIIBannerComponent" qualifier="bannerContainer" cardinality="one"/>
            <targetElement type="CustomSimpleItemComponent" qualifier="containerItems" cardinality="many" ordered="true"/>
        </relation>

        <relation code="BannerComponentII2CustomSimpleItemComponent" generate="true" localized="false" autocreate="true">
            <sourceElement type="BannerComponentII" qualifier="bannerContainer" cardinality="one"/>
            <targetElement type="CustomSimpleItemComponent" qualifier="containerItems" cardinality="many" ordered="true"/>
        </relation>





        <relation code="CBComponent2CSIComponent" generate="true" localized="false" autocreate="true">
            <sourceElement type="CBComponent" qualifier="BC" cardinality="one"/>
            <targetElement type="CSIComponent" qualifier="CI" cardinality="many" ordered="true"/>
        </relation>


    </relations>

    <itemtypes>
        <typegroup name="CMS">
            <itemtype code="TcSplitViewBannerComponent" extends="AbstractResponsiveBannerComponent"
                      jaloclass="com.taloscommerce.core.jalo.TcSplitViewBannerComponent">
                <attributes>
                    <attribute qualifier="title" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="content" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>

        <typegroup name="Training Core Items">
            <itemtype code="DocumentType">
                <deployment table="DocumentTypes" typecode="12000"/>
                <attributes>
                    <attribute qualifier="code" type="java.lang.String">
                        <modifiers unique="true" initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="name" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
                <indexes>
                    <index name="codeIdx" unique="true">
                        <key attribute="code"/>
                    </index>
                </indexes>
            </itemtype>

            <itemtype code="ReferredCustomer">
                <deployment table="ReferredCustomers" typecode="12001"/>
                <attributes>
                    <attribute qualifier="email" type="java.lang.String">
                        <modifiers unique="true" initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="documentType" type="DocumentType">
                        <modifiers initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="documentNumber" type="java.lang.String">
                        <modifiers initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="firstName" type="java.lang.String">
                        <modifiers initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="lastName" type="java.lang.String">
                        <modifiers initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
                <indexes>
                    <index name="emailIdx" unique="true">
                        <key attribute="email"/>
                    </index>
                </indexes>
            </itemtype>
        </typegroup>

        <typegroup name="Electronics">
            <itemtype code="ElectronicsColorVariantProduct" extends="VariantProduct"
                      jaloclass="com.taloscommerce.core.jalo.ElectronicsColorVariantProduct">
                <attributes>
                    <attribute qualifier="color" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color of the product.</description>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>
        <typegroup name="CustomList">
            <itemtype code="CustomProductList" extends="GenericItem">
                <deployment table="CustomProductList" typecode="12002"/>
                <attributes>
                    <attribute qualifier="id" type="java.lang.String">
                        <description>Id of the list</description>
                        <modifiers optional="false" initial="true" unique="true"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="name" type="java.lang.String">
                        <description>Name of the list</description>
                        <modifiers initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="description" type="java.lang.String">
                        <description>Description of the list</description>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>


        <typegroup name="CustomFooter">
            <itemtype code="CustomFooterNavNode" extends="CMSNavigationNode">
                <attributes>
                    <attribute qualifier="media" type="localized:Media">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="CustomFooterBottom" extends="FooterNavigationComponent">
            </itemtype>
        </typegroup>
        <typegroup name="footerSocialNetwork">
            <itemtype code="FooterSocialLink" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.FooterSocialLink"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="url" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="text" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="media" type="Media">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

           <itemtype code="FooterSocialLinkContainer" extends="SimpleCMSComponent"
                     jaloclass="com.taloscommerce.core.jalo.FooterSocialLinkContainer"
                      autocreate="true" generate="true">
            </itemtype>
        </typegroup>

        <typegroup name="extraPages">
            <itemtype code="SimpleItem" extends="SimpleCMSComponent" jaloclass="com.taloscommerce.core.jalo.SimpleItem"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="title" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="media" type="Media">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="SimpleItemContainer" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.SimpleItemContainer"
                      autocreate="true" generate="true">
            </itemtype>

            <itemtype code="SimpleItemComponent" extends="SimpleCMSComponent" jaloclass="com.taloscommerce.core.jalo.SimpleItemComponent"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="title" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="media" type="Media">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="CardComponent" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.CardComponent"
                      autocreate="true" generate="true">
            </itemtype>

            <itemtype code="CustomSimpleItemComponent" extends="SimpleCMSComponent" jaloclass="com.taloscommerce.core.jalo.CustomSimpleItemComponent"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="title" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="description" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="media" type="Media">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="CustomBannerComponent" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.CustomBannerComponent"
                      autocreate="true" generate="true">
            </itemtype>

            <itemtype code="CustomIIBannerComponent" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.CustomIIBannerComponent"
                      autocreate="true" generate="true">
            </itemtype>

            <itemtype code="BannerComponentII" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.BannerComponentII"
                      autocreate="true" generate="true">
            </itemtype>





            <itemtype code="CSIComponent" extends="SimpleCMSComponent" jaloclass="com.taloscommerce.core.jalo.CSIComponent"
                      autocreate="true" generate="true">
                <attributes>
                    <attribute qualifier="title" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="description" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="media" type="Media">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="CBComponent" extends="SimpleCMSComponent"
                      jaloclass="com.taloscommerce.core.jalo.CBComponent"
                      autocreate="true" generate="true">
            </itemtype>


        </typegroup>
    </itemtypes>
</items>
